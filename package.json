{
	"name": "weebsearch",
	"scripts": {
		"start": "nodemon ./src/index.js",
		"subs": "ts-node ./src/index.ts",
		"dev": "npm-run-all --parallel start playground",
		"test": "tsc && jest --config=jest.config.js --env node",
		"debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect ./src/api/index.ts",
		"playground": "graphql playground",
		"build": "rimraf dist && tsc",
		"typedoc": "typedoc --out docs/ ./src/worker",
		"lint:worker": "tslint --project ./src/worker"
	},
	"dependencies": {
		"ass-compiler": "0.0.9",
		"axios": "^0.18.0",
		"dotenv": "^6.0.0",
		"fuse.js": "^3.3.0",
		"glob": "^7.1.3",
		"graphql-request": "^1.8.2",
		"jsonwebtoken": "8.3.0",
		"pg": "^7.7.1",
		"ramda": "^0.25.0",
		"redis": "^2.8.0",
		"robots-txt-parse": "^1.0.1",
		"unpack-all": "0.0.4",
		"winston": "^3.1.0",
		"winston-daily-rotate-file": "^3.5.1"
	},
	"devDependencies": {
		"@types/dotenv": "^4.0.3",
		"@types/fuzzyset.js": "0.0.1",
		"@types/jest": "^23.3.9",
		"@types/jsonwebtoken": "^7.2.8",
		"@types/node": "^10.12.4",
		"@types/ramda": "^0.25.41",
		"@types/redis": "^2.8.6",
		"dotenv-cli": "1.4.0",
		"jest": "^23.6.0",
		"nodemon": "1.18.4",
		"npm-run-all": "4.1.3",
		"prettier": "^1.15.1",
		"prettier-tslint": "^0.4.0",
		"rimraf": "2.6.2",
		"ts-node": "6.2.0",
		"tslint": "^5.11.0",
		"tslint-config-prettier": "^1.15.0",
		"tslint-eslint-rules": "^5.4.0",
		"tslint-immutable": "^4.9.1",
		"typedoc": "^0.13.0",
		"typescript": "^3.1.6"
	}
}
